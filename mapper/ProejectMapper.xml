<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lc.lcserve.mapper.ProjectMapper">

    <insert id="insertRetunId" useGeneratedKeys="true" keyProperty="pid">
        insert into project
        (
            name
        )
        values(
            #{name}
        )
    </insert>

</mapper>
